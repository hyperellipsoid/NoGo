			return possible_pos_list[rand() % possible_pos_list.size()];
		}
	    else
		    return available_list[rand() % num_available];
}
	
int judge_eye_breaker(int r,int c,int flag_side)//返回1表示优先下此处
{
	int Qi = 4;
	//int suround_en = 0;
	int	suround_fr = 0;
	if (check(r, c)) {
		for (int i = 0; i < 4; i++) {
			int r1 = r + MOVE[i][0];
			int c1 = c + MOVE[i][1];
			if (check(r1, c1)) {
				if (board[r1][c1] != 0) {
					Qi--;
					if (board[r1][c1] == flag_side)
						suround_fr++;
					//else if (board[r1][c1] != flag_side)
						//suround_en++;
				}
			}
			else {
				Qi--;
			}
		}
	}
	if (Qi == 1&&suround_fr==0)
		return 1;
	else
		return 0;
}

int move_eye_breaker(int r, int c, int flag_side)
{
	for (int i = 0; i < 4; i++) {
		int r1 = r + MOVE[i][0];
		int c1 = c + MOVE[i][1];
		if (judgement(r1, c1, flag_side)&&check(r1,c1)) {
			return r1 * edge + c1;
		}
	}
	return MININT;
}

